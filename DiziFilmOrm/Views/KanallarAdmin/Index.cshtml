@model IEnumerable<DiziFilmModel.Models.Kanallar>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.KanalAdi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KanalSahibi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KurulusYili)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KanalAdi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KanalSahibi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KurulusYili)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.KanalID">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.KanalID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
